package com.kt.yapp.domain;

import java.util.Date;

import com.kt.yapp.push.Notification;

import io.swagger.annotations.ApiModelProperty;
import lombok.Data;

/**
 * push 데이터
 */
@Data
public class PushMessage {
	
	@ApiModelProperty(value = "MESSAGE_ID")
	private Long id;
	
	@ApiModelProperty(value = "TITLE")
	private String title;
	
	@ApiModelProperty(value = "CONTENT")
	private String content;
	
	@ApiModelProperty(value = "REG_DATE")
	private Date regDate;
	
	@ApiModelProperty(value = "DISPLAY_TYPE")
	private int displayType;
	
	@ApiModelProperty(value = "MENU_TYPE")
	private String menuType;
	
	@ApiModelProperty(value = "VENDOR_CODE")
	private String vendorCode;
	
	@ApiModelProperty(value = "LINK_URL")
	private String linkUrl;
	
	@ApiModelProperty(value = "LOGIN_YN")
	private String loginYn;
	
	@ApiModelProperty(value = "badge")
	private int badge;
	
	@ApiModelProperty(value = "notification")
	private Notification notification;

}

    position: fixed;
    width:90%;
    min-height: 200px;
    padding-top: 85px;
    padding-bottom: 50px;
    background:#fafafa url(../img/img_5g.png) center 27px no-repeat;
    background-size: auto 32px;
    box-sizing: border-box;
    z-index: 99999;
}
.alert_yteen {
    position: fixed;
    width:90%;
    min-height: 200px;
    padding-top: 85px;
    padding-bottom: 50px;
    background:#fafafa url(../img/img_yteen.png) center 27px no-repeat;
    background-size: auto 32px;
    box-sizing: border-box;
    z-index: 100000;
}
.alert.travel{
	background:#fafafa url(../img/group-travel_5.png) center 5px no-repeat;
    background-size: auto 80px;
}
.alert.shopping{
	background:#fafafa url(../img/group-shopping_5.png) center 5px no-repeat;
    background-size: auto 80px;
}
.alert.contents{
	background:#fafafa url(../img/group-contents_5.png) center 5px no-repeat;
    background-size: auto 80px;
}
.alert.culture{
	background:#fafafa url(../img/group-culture_5.png) center 5px no-repeat;
    background-size: auto 80px;
}

.alert .pop_body {
	position:relative;
    padding: 30px 15px 45px;
    text-align: center;
}
.alert .pop_body:after { content:""; position:absolute; left:50%; top:0; width:90%; height:1px; background:#efefef; box-sizing:border-box; transform:translate(-50%,0); -webkit-transform:translate(-50%,0); -ms-transform:translate(-50%,0);}
.alert .pop_body .text strong {font-weight: 500;color: #000;}
.alert .pop_body .warning {padding-top:15px;font-size: 11px;color: #999;}

.alert_yteen .pop_body {
	position:relative;
    padding: 30px 15px 45px;
    text-align: center;
}
.alert_yteen .pop_body:after { content:""; position:absolute; left:50%; top:0; width:90%; height:1px; background:#efefef; box-sizing:border-box; transform:translate(-50%,0); -webkit-transform:translate(-50%,0); -ms-transform:translate(-50%,0);}
.alert_yteen .pop_body .text strong {font-weight: 500;color: #000;}
.alert_yteen .pop_body .warning {padding-top:15px;font-size: 11px;color: #999;}

.event {
    position: fixed;
    width: 100%;
    height: 100%;
    background: #fff;
    z-index: 99999;
    height: inherit;
    overflow: auto;
}
.event .pop_footer {position:fixed; left:0; bottom:0; width:100%; z-index:10;}
.event .event_title {
    height: 85px;
    background:no-repeat center;
    background-size: auto 100%;
}
.event .event_title.group-travel { background-image:url(../img/group-travel_5.png);}
.event .event_title.group-shopping { background-image: url(../img/group-shopping_5.png);}
.event .event_title.group-culture { background-image: url(../img/group-culture_5.png);}
.event .event_title.group-contents { background-image: url(../img/group-contents_5.png);}
.event .event_title.group-yteen { background-image: url(../img/group-yteen_5.png);}

.event .event_choice {
    padding:40px 20px 30px;
    /* min-height: 390px; */
    background: #f4f4f4 url(../img/event_bg.png) center top no-repeat;background-size: 320px;
	font-size:0;
	text-align:center;
}
.event .event_choice .title {
    line-height: 14px;
    background: #f4f4f4 url(../img/event_titleBg.png) center center repeat-x;background-size: auto 50%;
    text-align: center;
	font-size:14px;
}
.event .event_choice .title span {
    padding:0 10px;
    line-height: 14px;
    background: #f4f4f4;
    font-weight: 200;
}
.event .event_choice .chk {
    display: inline-block;
    margin:30px 10px 0;
    width: 120px;
}
.event .event_choice .chk input {
    position: absolute;
    left: -9999px;
    display: none;
    width: 1px;
    height: 1px;
}
.event .event_choice .chk i {
    position: relative;
    display: inline-block;
    width: 120px;
    height: 120px;
    border-radius: 100%;
    background: #ddd;
}
.event .event_choice input:checked + i::after {
    position: inherit;
    display: block;
    content: '';
    width: 120px;
    height: 120px;
    border-radius: 100%;
    background:url(../img/icon_check.png) center center no-repeat;background-size: auto 50%;
    z-index: 999;
}
.event .event_choice#shopping input:checked + i::after { background-color:rgba(23,198,198,0.8);}
.event .event_choice#culture input:checked + i::after { background-color:rgba(246,126,118,0.8);}
.event .event_choice#travel input:checked + i::after { background-color:rgba(37,185,232,0.8);}
.event .event_choice#contents input:checked + i::after { background-color:rgba(255,211,56,0.8);}


.event .event_choice .chk i img {
    position: absolute;
    top:50%;
    left:50%;
    transform: translate(-50%, -50%);
}
.event .event_choice .chk span {
    display: block;
	margin-top:6px;
    font-size: 13px;
    text-align: center;
    color:#000;
	letter-spacing: -0.05em;
}
.event .event_choice.choice3 {
    text-align: center;
}
.event .event_choice.choice3 .chk {
    width: 86px;
    margin:30px 4px 0;
}
.event .event_choice.choice3 .chk:first-child {margin-left: 0px;}
.event .event_choice.choice3 .chk i,
.event .event_choice.choice3 input:checked + i::after {
    width: 86px;
    height: 86px;
}

.event .text { display:none; padding:30px 0 20px;text-align: center;font-weight: 500;color: #000;}
.event #form_user_info { display:none;}
.event form legend {
    padding:0 20px;
    width:calc(100% - 40px);
    height: 37px;
    line-height: 37px;
    border-top: 1px solid #dfdfdf;
    border-bottom: 1px solid #dfdfdf;
    font-size: 14px;
    background: #f6f6f6;
}
.event form .form_inner {padding:20px 20px 80px;}
.event form .form_inner::after {content:""; display:block; clear: both;}
.event form .form_inner label {float: left;width: 65px;height: 35px;line-height: 35px;color:#000;}
.event form .form_inner input {
    float: right;
    padding:0 10px;
    width: calc(100% - 65px);
    height: 35px;
    border: 1px solid #d4d4d4;
    color:#878787;
    box-sizing: border-box;
}
.event form .form_inner input::-webkit-input-placeholder { font-size:inherit; color:inherit;}
.event form .form_inner input.address {float: left; width: calc((100% - 70px) / 2);}
.event form .form_inner .btn_address {
    float: right;
    padding:0 10px;
    margin-top:10px;
    width: calc((100% - 70px) /2);
    height: 35px;
    border: 1px solid #878787;
    text-align: center;
    color:#636363;
    box-sizing: border-box;
    background: #fff;
}
.event form .form_inner label,
.event form .form_inner input {margin-top:10px;}
.event form .form_inner label:nth-child(1),
.event form .form_inner input:nth-child(2) {margin-top:0px;}

.pop_footer {position: absolute;bottom:0;width: 100%;height: 50px;}
.pop_footer .pop_btn {display: inline-block;float: left;width: 50%;height: 50px;color:#fff;}
.pop_footer .btn_cancel {background: #a9a9a9;}
.pop_footer .del_check {background: #36c2c0;}
.pop_footer .btn_ok {background: #36c2c0;}
.pop_footer .btn_ok1 {background: #36c2c0;}

.layerPop.info {
    position: absolute;
    top:3%;
    left:50%;
    width: 90%;
    height: calc(97% - 50px);
    background: #fff;
    z-index: 99999;
    transform: translate(-50%, 0%);
    /* height: inherit; */
    overflow: auto;
}
.layerPop.info .pop_footer {position: -webkit-sticky;position:sticky; bottom:0; width:100%; z-index:10;}
.layerPop.info .pop_footer .btn_cancel {display: block;width: 100%;height: 50px;font-size: 16px;color:#fff;background: #2bb7b4;}

body.popActive {
    overflow: hidden;
}







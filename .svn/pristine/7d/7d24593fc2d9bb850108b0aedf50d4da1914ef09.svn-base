.section .icon_crown {
    position: absolute;
    top:10px;
    left:10px;
    width: 16px;
    height: 11px;
    background:no-repeat center;
	background-image:url(../img/icon_crown.png);
    background-size: 100%;
}
.section .icon_crown.off { background-image:url(../img/icon_crown_off.png);}
.section .inner_info {
    position: absolute;
    top:-1px;
    left:-1px;
    padding:2.5px 5px;
    color:#fff;
    font-size: 12px;
}
.section.no-thema .inner_info { background:#e5e5e5;}
.section.group-shopping .inner_info {background:#17c6c6;}
.section.group-travel .inner_info {background:#24aae2;}
.section.group-contents .inner_info {background:#ffd338;}

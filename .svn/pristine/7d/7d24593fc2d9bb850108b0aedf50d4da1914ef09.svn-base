.section .popApp {
    position: absolute;
    bottom:-1px;
    right:-1px;
    width: 30px;
    height: 30px;
    border: 1px solid #e5e5e5;
    background: #fff;
}
.section .popApp.open {background:#fff url(../img/icon_open.png) center center no-repeat;background-size: 50%;}
.section .popApp.close {background:#fff url(../img/icon_close.png) center center no-repeat;background-size: 50%;}

/* Reset */
html,body,h1,h2,h3,h4,h5,h6,div,p,blockquote,pre,code,address,ul,ol,li,menu,nav,section,article,aside,
dl,dt,dd,table,thead,tbody,tfoot,label,caption,th,td,form,fieldset,legend,hr,input,button,textarea,object,figure,figcaption {margin:0;padding:0; outline:none;}
html, body{width:100%; box-sizing:border-box !important;}
html{-webkit-touch-callout:none; -webkit-user-select:none; -webkit-tap-highlight-color:rgba(0, 0, 0, 0); position: relative; min-height: 100%; -webkit-font-smoothing: antialiased; -moz-osx-font-smoothing: grayscale;}
body{width:100%; background:#fff; min-width:320px; min-height:100%; -webkit-text-size-adjust:none;word-wrap:break-word;word-break:break-all;letter-spacing:-0.05em;}
body,input,select,textarea,button {border:none;font-size:12px;color:#000;}
ul,ol,li{list-style:none;}
table{width:100%;border-spacing:0;border-collapse:collapse;}
img,fieldset{border:0;}
address,caption,cite,code,em{font-style:normal;font-weight:normal;}
label,img,input,select,textarea,button{vertical-align:middle;}
caption,legend{line-height:0;font-size:1px;overflow:hidden;}
.hide { display:none;}
hr{display:none;}
main,header,section,nav,footer,aside,article,figure{display:block;}
a{color:#000;text-decoration:none;}
 * {-webkit-tap-highlight-color:rgba(0,0,0,0) !important;tap-highlight-color:rgba(0,0,0,0) !important;}
a:link,a:hover,a:active{text-decoration:none;}
.clear:after { content: ""; display:block; clear:both;}
select { display:inline-block; /* for Firefox */ -moz-appearance: none;	/* for Chrome */  -webkit-appearance: none; -webkit-border-radius: 0; -moz-border-radius:0; border-radius:0;}
/* For IE10 */
select::-ms-expand { display: none;}
input[type=text], input[type=password], input[type=search], input[type=number], input[type=email], input[type=tel], input[type=date] { padding:0; -webkit-border-radius:0; -moz-border-radius:0; border-radius:0;}

/* common */
body { overflow:hidden;}
input[type="radio"] { position:absolute; left:-30000%;}
.btn { display:inline-block; font-size:0; vertical-align:top;}
.typeGreen { background-color:#2bb7b3 !important;}
.typeGray { background-color:#dbdbdb !important;}
.inner { position:relative; padding:0 5%;}
.btm { position:fixed; left:0; bottom:0; padding:0 5% 20px; width:100%; background:#fff; text-align:center; font-size:0; box-sizing:border-box;}
.btm.typeHide { display:none;}
.btm .btn { width:100%; height:56px; border-radius:56px; text-align:center; font-size:18px; color:#fff;}
.btm.col2 .btn { margin-left:4%; width:48%; vertical-align:top;}
.btm.col2 .btn:first-child { margin:0;}
.btm.col2 .btn.small { width:28%;}
.btm.col2 .btn.large { margin-left:2%; width:70%;}

.yteenGuide { position:fixed; left:0; top:0; width:100%; border-bottom:1px solid #f4f4f4;}
.yteenGuide .inner { background:#f6f6f6;}
.yteenGuide .inner:first-child { position:relative; padding:20px 13%; background:#fafafa;}
.yteenGuide .txt { line-height:1.3; font-size:14px; color:#7b7b7b;}
.yteenGuide .state { padding:12px 3%; border-top:1px solid #efefef; font-size:16px; color:#707070;}

.yteenContent { overflow:auto; padding:20px 0 30px; box-sizing:border-box;}
.yteenContent > .inner { min-height:100%;}
.yteenContent .visualBg { overflow:hidden; position:absolute; left:0; top:0; width:100%; height:100%; background:url(../images/bgYteen.png) no-repeat center; background-size:90% auto;}
.yteenContent .btnArea .btn { width:100%; height:56px; border-radius:56px; line-height:56px; text-align:center; font-size:20px; color:#fff;}
.yteenContent .boxType1 { position:relative; padding-bottom:70%; height:0; border:1px solid #e5e5e5; border-radius:20px; background:url(../images/bgYteen2.png) no-repeat center; background-size:90% auto; box-sizing:border-box;}
.yteenContent .boxType1 .btn { position:absolute; left:50%; bottom:8%; width:40%; min-height:56px; border-radius:56px; font-size:17px; color:#fff; transform:translateX(-50%); -webkit-transform:translateX(-50%);}
.yteenContent .boxType2 .topArea { position:relative; padding-bottom:35%; height:0; border:1px solid #e5e5e5; border-bottom:0; border-top-left-radius:20px; border-top-right-radius:20px; background:url(../images/bgYteen3.png) no-repeat center; background-size:95% auto; box-sizing:border-box;}
.yteenContent .boxType2 .topArea.complete:after { content:""; position:absolute; left:3%; top:5%; padding-bottom:12%; width:12%; height:0; background:url(../images/label1.png) no-repeat center; background-size:100% auto;}

.yteenContent .boxType2 .circleContainer { position:relative; padding-top:10%;}
.yteenContent .boxType2 .circleArea { display:none; position:relative; margin:0 auto; padding-bottom:70%; width:70%; height:0; background:no-repeat center; background-size:100% auto;}
.yteenContent .boxType2 .circleArea.finish { opacity:0.3;}
.yteenContent .boxType2 .circleArea.member0 { background-image:url(../images/bgCircle0.png);}
.yteenContent .boxType2 .circleArea.member1 { background-image:url(../images/bgCircle1.png);}
.yteenContent .boxType2 .circleArea.member2 { background-image:url(../images/bgCircle2.png);}
.yteenContent .boxType2 .circleArea.member3 { background-image:url(../images/bgCircle3.png);}
.yteenContent .boxType2 .circleArea.member4 { background-image:url(../images/bgCircle4.png);}
.yteenContent .boxType2 .circleArea .user { display:none; position:absolute; width:25%; background-color:transparent;}
.yteenContent .boxType2 .circleArea .user.leader:after { content:""; position:absolute; right:-6%; top:-2px; padding-bottom:33%; width:33%; height:0; background:url(../images/icoCrown.png) no-repeat center; background-size:100% auto;}
.yteenContent .boxType2 .circleArea .user.nth1 { left:50%; top:-10%; transform:translate(-50%, 0); -webkit-transform:translate(-50%, 0);}
.yteenContent .boxType2 .circleArea .user.nth2 { left:-11%; top:22%;}
.yteenContent .boxType2 .circleArea .user.nth3 { left:5%; bottom:0;}
.yteenContent .boxType2 .circleArea .user.nth4 { right:5%; bottom:0;}
.yteenContent .boxType2 .circleArea .user.nth5 { right:-11%; top:22%;}

/* 2020.06.04 전민재 수정 Start */
.yteenContent .boxType2 .circleArea .user .thumb { overflow:hidden; position:relative; border-radius:100%;}
/* 2020.06.04 전민재 수정 End */

.yteenContent .boxType2 .circleArea .user.active .thumb:after { content:""; position:absolute; left:0; top:0; width:100%; height:100%; border:3px solid #2bb7b3; border-radius:100%; box-sizing:border-box;}
.yteenContent .boxType2 .circleArea .user .thumb img { width:100%; height:100%;}
.yteenContent .boxType2 .circleArea .user p { padding-top:4px; text-align:center; font-size:11px; color:#636363;}
.yteenContent .boxType2 .circleArea .user.active p { color:#2bb7b3;}

.yteenContent .boxType2 .btmArea { padding:6% 0; background:#2BB7B3; border-bottom-left-radius:20px; border-bottom-right-radius:20px; text-align:center;}
.yteenContent .boxType2 .btmArea .info { font-size:15px; color:#fff;}
.yteenContent .boxType2 .btmArea .info .count { font-weight:bold;}
.yteenContent .boxType2 .btmArea .btnPlus { margin-top:3%; width:30%; height:28px; border:1px solid #fff; border-radius:28px; text-align:center; font-size:13px; color:#fff; box-sizing:border-box;}
.yteenContent .boxType2 .btnExit { display:none; position:absolute; right:0; top:0; width:62px; height:24px; background:#DBDBDB; border-radius:24px; text-align:center; font-size:12px; color:#fff;}

/* Responsive */
@media (max-width:400px){
	.yteenContent .btnArea .btn { font-size:18px;}
	.btm .btn { font-size:16px;}
	.yteenGuide .txt { font-size:13px;}
	.yteenGuide .state { font-size:15px;}
	.yteenContent .boxType1 .btn { font-size:16px;}
	.yteenContent .boxType2 .btmArea .info { font-size:14px;}
	.yteenContent .boxType2 .btmArea .btnPlus { font-size:12px;}
	.yteenContent .boxType2 .circleArea .user p { font-size:10px;}
}

@media (max-width:359px){
	.btm .btn { font-size:15px;}
}
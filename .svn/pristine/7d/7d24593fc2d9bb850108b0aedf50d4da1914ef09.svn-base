@charset "utf-8";

@import url('notosanskr.css');
* {font-family: 'Noto Sans KR', sans-serif;}


html, body { height: 100%;}
#wrap { width:100%; height: 100%; margin:0;font-size: 14px; overflow:auto;}
.container { position: relative;}

.info_title {
    padding: 140px 20px 30px;
    background:#fafafa url(../img/bg_5g.png) center 30px no-repeat;
    background-size: auto 32%;
    border-bottom: 1px solid #b7b7b7;
}
.info_title > div { position:relative; padding:9px 18px; border:1px solid #2bb7b3; box-sizing:border-box;}
.info_title > div:after { content:""; position:absolute; left:-1px; top:50%; z-index:1; width:2px; height:80%; background:#fafafa; transform:translate(0,-50%); -webkit-transform:translate(0,-50%);}
.info_title > div:before { content:""; position:absolute; right:-1px; top:50%; z-index:1; width:2px; height:80%; background:#fafafa; transform:translate(0,-50%); -webkit-transform:translate(0,-50%);}
.info_title p { padding-left:48px; margin-top:7px; line-height:20px; color:#2bb7b3; font-size:13px; letter-spacing:-0.05em;}
.info_title p:first-child { margin-top:0px;}
.info_title .tit {display: inline-block;margin-left: -48px;width: 48px; color:#2bb7b3; font-weight:bold;}

.listTop_info {
    display: none;
    position: fixed;
    top:0;
    padding: 20px 45px 20px 20px;
    width: calc(100% - 65px);
    background:#f4f4f4;
    border-bottom: 1px solid #efefef;
    z-index: 9999;
}
.listTop_info p {
    width: 100%;
    height: 50px;
    font-size: 12px;
    overflow: hidden;
}
/* .listTop_info.open p {height: auto;}
.listTop_info.close p {height: 36px;} */

.listTop_info .btn_info {
    position: absolute;
    top:50%;
    right:0;
    margin-top:-20px;
    width: 45px;
    height: 40px;
}
.listTop_info .btn_info.open {background:url(../img/icon_s_open.png) center center no-repeat;background-size: 10px;}
.listTop_info .btn_info.close {background:url(../img/icon_s_close.png) center center no-repeat;background-size: 10px;}

.content {padding:20px;}


.section .inner_member {display: block;margin-top:10px;text-align: center;}
.section.no-thema .inner_member span { color:#e5e5e5;}
.section.group-shopping.dim .inner_member span { color:#17c6c6;}
.section.group-travel.dim .inner_member span { color:#24aae2;}
.section.group-contents.dim .inner_member span { color:#ffd338;}
.section.group-culture.dim .inner_member span { color:#f26763;}

.section.member5 .inner_member {margin-top:10px;}

/* 방장용 */
.section.owner .inner_member {display: none;margin: 20px 0 25px 0;}
.section.owner .inner_member .owner {
    display: block;
    width: 58px;
    height: 58px;
    padding-top:30px;
    border: 1px solid #e5e5e5;
    border-radius:15px;
    margin:0 auto;
    background:#fff url(../img/icon_crown.png) center 12px no-repeat;background-size: 30%;
    box-sizing: border-box;
}

.section.owner .inner_member .member {
    display: inline-block;
    width: 58px;
    height: 58px;
    margin:0 1px;
    margin-top:15px;
    border: 1px solid #e5e5e5;
    border-radius:15px;
    box-sizing: border-box;
    vertical-align: top;
    line-height: 58px;
    background:#fff url(../img/bg_no-member.png) center center no-repeat;background-size: 100%;
}
.section.group-travel.owner .inner_member .member {background:#fff url(../img/bg_no-member_travel.png) center center no-repeat;background-size: 100%;}
.section.group-shopping.owner .inner_member .member {background:#fff url(../img/bg_no-member_shopping.png) center center no-repeat;background-size: 100%;}
.section.group-culture.owner .inner_member .member {background:#fff url(../img/bg_no-member_culture.png) center center no-repeat;background-size: 100%;}
.section.group-contents.owner .inner_member .member {background:#fff url(../img/bg_no-member_contents.png) center center no-repeat;background-size: 100%;}

.section.owner .inner_member span:nth-child(2)  {margin-left:0}
.section.owner .inner_member span:nth-child(5) {margin-right:0}
.section.owner .inner_member .member.in {background:#fff;}
.section.group-shopping.owner .inner_member .owner,
.section.group-shopping .inner_member .member.in {border-color: #17c6c6;color:#17c6c6;}
.section.group-travel.owner .inner_member .owner,
.section.group-travel .inner_member .member.in {border-color: #24aae2;color:#24aae2;}
.section.group-contents.owner .inner_member .owner,
.section.group-contents .inner_member .member.in {border-color: #ffd338;color:#ffd338;}
.section.group-culture.owner .inner_member .owner,
.section.group-culture .inner_member .member.in {border-color: #f26763;color:#f26763;}

.section .inner_body .inner_btn {
    display: inline-block;
    width: calc(100% - 32px);
    height: 36px;
    line-height: 36px;
    margin:0 15px;
    margin-top:15px;
    border: 1px solid #999; 
    background: #fff;
    color:#999;
	text-align:center;
}
.section.group-shopping.member5 .inner_body .inner_btn {border:0;background: #17c6c6;color:#fff;}
.section.group-travel.member5 .inner_body .inner_btn {border:0;background: #24aae2;color:#fff;}
.section.group-contents.member5 .inner_body .inner_btn {border:0;background: #ffd338;color:#fff;}
.section.group-culture.member5 .inner_body .inner_btn {border:0;background: #f26763;color:#fff;}


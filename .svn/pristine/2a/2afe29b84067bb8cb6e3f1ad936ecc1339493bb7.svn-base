@charset "UTF-8";

/* reset */
html, body{width:100%; height:auto;}
*, body{margin:0; padding:0; font-weight:400; font-size:1rem; line-height:1.2; vertical-align:baseline; box-sizing:border-box; color:#111;}
p{line-height:1.6;}
li{list-style:none;}
img{max-width:100%;}
span{font:inherit}
a, a:hover, .btn:hover{text-decoration:none; color:inherit;}
a[disabled] {pointer-events: none;}
.h1, .h2, .h3, .h4, .h5, .h6, h1, h2, h3, h4, h5, h6{}
.clearfix:after{content:""; display:block; clear:both;}

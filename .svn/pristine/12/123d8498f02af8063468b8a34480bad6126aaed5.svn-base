/* radio custom */
.radio-custom-wrap{position:relative; padding-left:1.5rem}
input[type="radio"] {display:none;}
.radio-custom {position: absolute; top: 0; left: 0; height: 1rem; width: 1rem; background-color: #fff; border:1px solid #DADADA; border-radius: 50%;}
.radio-custom-wrap input:checked ~ .radio-custom {background-color: #00BBB4;}
.radio-custom:after {content: "";position: absolute;display: none;}
.radio-custom-wrap input:checked ~ .radio-custom:after {display: block;}
.radio-custom-wrap .radio-custom:after {top:50%; left:50%; transform:translate(-50%,-50%); width: 1rem; height: 1rem; background:url(../img/chk.png) no-repeat 57% 61%;}

/* checkbox custom */
.checkbox-custom-wrap{position:relative; padding-left:1.5rem}
input[type="checkbox"] {display:none;}
.checkbox-custom {position: absolute; top: 0; left: 0; height: 1rem; width: 1rem; background-color: #fff; border:1px solid #DADADA; border-radius: 50%;}
.checkbox-custom-wrap input:checked ~ .radio-custom {background-color: #00BBB4;}
.checkbox-custom:after {content: "";position: absolute;display: none;}
.checkbox-custom-wrap input:checked ~ .radio-custom:after {display: block;}
.checkbox-custom-wrap .radio-custom:after {top:50%; left:50%; transform:translate(-50%,-50%); width: 1rem; height: 1rem; background:url(../img/chk.png) no-repeat 57% 61%;}



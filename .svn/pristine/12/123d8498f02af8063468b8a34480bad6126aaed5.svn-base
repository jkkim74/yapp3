/* popup */
.cover{position:fixed; width:100%; height:100%; background-color:rgba(0,0,0,0.67)}
.popup{position:fixed; top:0; left:0; right:0; bottom:0;}
.popup p{line-height:1.4}
.popup p + .sub-con{margin-top:2%;}
.popup .hide-btn{position:absolute; top:6%; right:12%;}

.popup * + .scroll{margin-top:11%; /* height:4vh; overflow:auto; */}
.popup .scroll h6{font-size:0.876rem; font-weight:600;}
.popup .scroll h6 + ul{margin-top:4%;}
.popup .scroll > ul > li{margin-top:8%;}
.popup .scroll > ul > li:first-child{margin-top:0;}
.popup .scroll ul li{font-size:0.815rem; color:#7d7d7d;}
.popup .scroll ul.ul-disc li{list-style:disc; margin-left:7.5%;}
/*210910 추가 시작*/
.popup .scroll ul.ul-disc.type2 li{margin-top:3%;}
.popup .scroll ul.ul-disc.type2 li:first-child{margin-top:0;}
/*210910 추가 끝*/
.popup .resualt-box{padding:5%;}
.popup .pop-con * + .resualt-box{margin-top:8%}
.popup .pop-con img + p{margin-top:4%;}
.popup .pop-con ul + p{margin-top:4%;}
.popup .pop-con .radio-list-row ul{padding-right:8%;}
.popup .pop-con .radio-list-row ul .on img{border-radius:20px; border:2px solid #00BBB4}

.popup .pop-btn-wrap{font-size:0; position:absolute; bottom:0; left:0; right:0;}
.popup .pop-btn-wrap button{display:inline-block; padding:5%; font-size:0.876rem;}
.popup .pop-btn-wrap.bg-darkgray button.w50p{position:relative;}
.popup .pop-btn-wrap.bg-darkgray button.w50p:before{content:""; display:block; position:absolute; top:50%; transform:translatey(-50%); left:0; width:1px; height:1rem; background-color:#7D7D7D;}
.popup .pop-btn-wrap.bg-darkgray button.w50p.btn-high:before{top:40%;}

.popup .pop-ct{position:absolute; top:13%; bottom:10%; left:15%; right:15%; padding-bottom:77px; overflow:hidden; background-color:#fff; text-align:center;display:flex;}
.popup .pop-ct .pop-con{display:flex;flex-direction:column;flex:0 0 100%;}
.popup .pop-ct .pop-tit-con{padding-top:13%; padding-bottom:10%; border-bottom:1px dashed #aaa;flex:0 0 auto;}
.popup .pop-ct .pop-tit-con img + p{margin-top:4%}
.popup .pop-ct .pop-btn-wrap button.w100p{padding:6.5%;}
.popup .pop-content{padding:10% 8%;flex:1 1 35%;min-height:35%;overflow-y:scroll;}
.popup .pop-content::-webkit-scrollbar {width: 4px !important;}
.popup .pop-content::-webkit-scrollbar-thumb {background: #a9a9a9 !important;}
.popup .box-round p + button{margin-top:3%; margin-bottom:6%;}

.popup .pop-bt{position:fixed; bottom:0; left:0; right:0; background-color:#fff; text-align:center;}
.popup .pop-bt .pop-con{padding:9% 8%;}
.popup .pop-bt .pop-btn-wrap{position:static;}

/*210902 추가*/
.popup .pop-bt .con {margin-top: 5px;}
.popup .pop-bt .sub-con {display: block; margin-top: 15px;}

